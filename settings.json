{
  // JetBrainsMono, CaskaydiaCove, Hasklug, FiraCode, FantasqueSansMono
  // GohuFont uni14, Agave, Lilex, ProFont IIx, ProFontWindows
  // 'Monaspace Neon', 'Monaspace Argon', 'Monaspace Xenon', 'Monaspace Radon', 'Monaspace Krypton', Dank Mono
  // ------------------------------- EDITOR CONFIG -------------------------------
  "editor.fontFamily": "'FiraCode Nerd Font', monospace",
  "editor.fontWeight": "500",
  "editor.fontSize": 13,
  "editor.lineHeight": 1.5,
  // "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'calt', 'dlig'",
  "editor.fontLigatures": true,
  "editor.wordWrap": "on",
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.autoClosingBrackets": "always",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.accessibilitySupport": "on",
  "editor.renderWhitespace": "none",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.stickyScroll.enabled": true,
  "editor.scrollbar.horizontal": "hidden",
  // "editor.scrollbar.vertical": "hidden",
  "editor.renderLineHighlight": "all",
  // ------------------------------- WORKBENCH CONFIG -------------------------------
  "workbench.startupEditor": "none",
  "workbench.list.smoothScrolling": true,
  "workbench.preferredDarkColorTheme": "One Dark Darker",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "top",
  // "workbench.statusBar.visible": false,
  // "editor.minimap.enabled": false,
  "workbench.tree.enableStickyScroll": true,
  "workbench.colorCustomizations": {},
  "workbench.editorAssociations": {
    // "*.pdf": "latex-workshop-pdf-hook",
    "*.docx": "default",
    "*.png": "editor.excalidraw"
  },
  // ------------------------------- EXPLORER CONFIG -------------------------------
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "ace": "ace*",
    "tsconfig.json": "tsconfig*",
    "package.json": "package*, yarn*, pnpm*",
    ".env": ".env*"
  },
  // ------------------------------- TERMINAL CONFIG -------------------------------
  "terminal.integrated.fontFamily": "FiraCode Nerd Font, JetBrainsMono Nerd Font, monospace",
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyleInactive": "line",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.cursorStyle": "line",
  // ------------------------------- GITLENS CONFIG -------------------------------
  "gitlens.graph.minimap.enabled": false,
  "gitlens.graph.layout": "editor",
  "gitlens.graph.minimap.additionalTypes": ["localBranches"],
  // ------------------------------- TS CONFIG -------------------------------
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.semi": false
  },
  // ------------------------------- JS CONFIG -------------------------------
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.semi": false
  },
  // ------------------------------- PYTHON CONFIG -------------------------------
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  // ------------------------------- JSON CONFIG -------------------------------
  "[json]": {
    // "editor.defaultFormatter": "vscode.json-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // ------------------------------- JSONC CONFIG -------------------------------
  "[jsonc]": {
    // "editor.defaultFormatter": "vscode.json-language-features",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // ------------------------------- MARKDOWN CONFIG -------------------------------
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // ------------------------------- DOCKER COMPOSE CONFIG -------------------------------
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  // ------------------------------- FILE EXCLUDE CONFIG -------------------------------
  "files.exclude": {
    "**/.git": false,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    // "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  // ------------------------------- COPILOT CONFIG -------------------------------
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  // ------------------------------- JS/TS CONFIG -------------------------------
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  // ------------------------------- GIT CONFIG -------------------------------
  "git.confirmSync": false,
  // ------------------------------- WINDOW CONFIG -------------------------------
  "window.commandCenter": false,
  // ------------------------------- SECURITY CONFIG -------------------------------
  "security.workspace.trust.untrustedFiles": "open",
  // ------------------------------- ERROR LENS CONFIG -------------------------------
  "errorLens.enabled": true,
  // ------------------------------- SYMBOLS CONFIG -------------------------------
  "symbols.hidesExplorerArrows": false,
  // ------------------------------- UPDATE CONFIG -------------------------------
  "update.showReleaseNotes": false,
  // ------------------------------- LIVE SERVER CONFIG -------------------------------
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  // ------------------------------- OTHERS CONFIG -------------------------------
  "editor.experimental.asyncTokenization": true,
  "editor.guides.highlightActiveBracketPair": true,
  "editor.bracketPairColorization.enabled": true,
  "accessibility.signals.lineHasWarning": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.lineHasError": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.lineHasFoldedArea": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.lineHasBreakpoint": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.lineHasInlineSuggestion": {
    "sound": "off"
  },
  "accessibility.signals.terminalQuickFix": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.onDebugBreak": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.noInlayHints": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.taskCompleted": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.taskFailed": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.terminalCommandFailed": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.notebookCellCompleted": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.notebookCellFailed": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.diffLineInserted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineDeleted": {
    "sound": "off"
  },
  "accessibility.signals.diffLineModified": {
    "sound": "off"
  },
  "accessibility.signals.positionHasError": {
    "sound": "off",
    "announcement": "off"
  },
  "files.associations": {
    "*.php": "php"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": []
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "macos-modern",
  "[php]": {
    "editor.defaultFormatter": "DEVSENSE.phptools-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "mohd-akram.vscode-html-format"
  },
  "editor.linkedEditing": true,
  "accessibility.signals.progress": {
    "sound": "off",
    "announcement": "off"
  },
  "editor.suggestSelection": "first",
  "explorer.confirmPasteNative": false,
  "git.suggestSmartCommit": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "accessibility.signals.terminalCommandSucceeded": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.voiceRecordingStarted": {
    "sound": "off"
  },
  "excalidraw.theme": "dark",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.semi": false,
  "accessibility.signals.chatRequestSent": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.chatResponseReceived": {
    "sound": "off"
  },
  "accessibility.signals.clear": {
    "announcement": "off",
    "sound": "off"
  },
  "accessibility.signals.positionHasWarning": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.terminalBell": {
    "sound": "off",
    "announcement": "off"
  },
  "accessibility.signals.voiceRecordingStopped": {
    "sound": "off"
  },
  "workbench.colorTheme": "scepter (darker)",
  "[nginx]": {
    "editor.defaultFormatter": "ahmadalli.vscode-nginx-conf"
  },
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  }
}
